.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.hide-scroll::-webkit-scrollbar{
  display: none;
}
page{
  width: 100%;
  height: 100%;
}
.ellipsis{overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}